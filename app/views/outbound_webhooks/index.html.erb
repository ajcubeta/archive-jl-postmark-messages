<h1><%= @title %></h1>

<table>
  <thead>
    <tr>
      <th>Payload</th>
      <th>Webhook Type</th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @webhooks.each do |webhook| %>
      <tr>
        <td><%= webhook.payload %></td>
        <td><%= webhook.webhook_type %></td>
        <td><%= link_to 'Show', webhook %></td>
      </tr>
    <% end %>
  </tbody>
</table>

